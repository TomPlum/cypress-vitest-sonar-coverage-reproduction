{
  "name": "cypress-vitest-sonar-coverage-reproduction",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "test:unit": "vitest src",
    "test:unit:ci": "rm -rf coverage-unit-dir && vitest --coverage --coverage.enabled=true --allowOnly",
    "preview": "vite preview",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cypress:ct:run:ci": "rm -rf coverage-cypress-dir && NODE_ENV=test npx cypress run --component --browser=chrome --env COVERAGE=true"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.6",
    "@eslint/js": "^9.33.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cypress": "^15.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-istanbul": "^7.1.0",
    "vitest": "^3.2.4"
  }
}
